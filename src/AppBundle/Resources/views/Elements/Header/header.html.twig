        <div class="container">
            <div class="inner-banner-dec">
                <ol class="breadcrumb">
                    {% for item in breadcrumbs %}
                        <li{% if loop.last %} class="active"{% endif %}>
                            {% if item.url is defined %}<a href="{{ item.url }}">{% endif %}
                                {{ item.name }}
                            {% if item.url is defined %}</a>{% endif %}
                        </li>
                    {% endfor %}
                </ol>
            </div>
        </div>
    </div>
