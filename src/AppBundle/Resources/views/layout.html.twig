{% spaceless %}
<!DOCTYPE html>
<html lang="ru">
    <head>
        <link rel="dns-prefetch" href="//bookary.ru">
        {% block title %}{{ sonata_seo_title() }}{% endblock %}
        {% block meta %}
            <meta http-equiv="X-UA-Compatible" content="IE=edge">
            <meta name="viewport" content="width=device-width, initial-scale=1">
            <meta name="yandex-verification" content="330cbf6ebd373675" />
            {{ sonata_seo_metadatas() }}
        {% endblock meta %}

        {% stylesheets
            '@AppBundle/Resources/public/css/bootstrap.min.css'
            '@AppBundle/Resources/public/css/font-awesome.min.css'
            '@AppBundle/Resources/public/css/svg.css'
            '@AppBundle/Resources/public/css/widget.css'
            '@AppBundle/Resources/public/css/typography.css'
            '@AppBundle/Resources/public/css/shortcodes.css'
            '@AppBundle/Resources/public/css/style.css'
            '@AppBundle/Resources/public/css/color.css'
            '@AppBundle/Resources/public/js/dl-menu/component.css'
            '@AppBundle/Resources/public/css/responsive.css'
            '@AppBundle/Resources/public/css/ads.css'
            filter='scssphp' output='compiled/css/styles.css' %}
            <link rel="stylesheet" href="{{ asset_url }}" />
        {% endstylesheets %}

        {% block stylesheets %}{% endblock %}

        {% block favicons %}
            <link rel="shortcut icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
            <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
        {% endblock %}

        {{ ad_by_position(constant('AppBundle\\Entity\\Ads::POSITION_HEADER'))|raw }}
    </head>

    <body>
        <div class="kode_wrapper">
            {%- block header %}
                {% include '@App/Elements/Header/header.html.twig' %}
            {%- endblock header %}

            {% block body %}{% endblock %}

            {%- block footer %}
                {% include '@App/Elements/Footer/footer.html.twig' %}
            {%- endblock footer %}

            {% javascripts
                '@AppBundle/Resources/public/js/jquery.js'
                '@AppBundle/Resources/public/js/bootstrap.min.js'
                '@AppBundle/Resources/public/js/waypoints-min.js'
                '@AppBundle/Resources/public/js/dl-menu/modernizr.custom.js'
                '@AppBundle/Resources/public/js/dl-menu/jquery.dlmenu.js'
                '@AppBundle/Resources/public/js/base.js'
                '@AppBundle/Resources/public/js/jquery.accordion.js'
                filter='?jsqueeze' output='compiled/js/scripts.js' %}
                <script src="{{ asset_url }}"></script>
            {% endjavascripts %}

            {% block javascripts %}{% endblock %}
        </div>

        {% block analytics %}
            <script>
                (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
                    (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
                    m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
                })(window,document,'script','https://www.google-analytics.com/analytics.js','ga');
                ga('create', 'UA-40513359-9', 'auto');
                ga('send', 'pageview');
            </script>
            <script type="text/javascript"> (function (d, w, c) { (w[c] = w[c] || []).push(function() { try { w.yaCounter42824504 = new Ya.Metrika({ id:42824504, clickmap:true, trackLinks:true, accurateTrackBounce:true, webvisor:true }); } catch(e) { } }); var n = d.getElementsByTagName("script")[0], s = d.createElement("script"), f = function () { n.parentNode.insertBefore(s, n); }; s.type = "text/javascript"; s.async = true; s.src = "https://mc.yandex.ru/metrika/watch.js"; if (w.opera == "[object Opera]") { d.addEventListener("DOMContentLoaded", f, false); } else { f(); } })(document, window, "yandex_metrika_callbacks"); </script> <noscript><div><img src="https://mc.yandex.ru/watch/42824504" style="position:absolute; left:-9999px;" alt="" /></div></noscript>
        {% endblock %}
    </body>
</html>
{% endspaceless %}