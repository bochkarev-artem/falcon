{
    "name": "artem.bochkarev/peregrine_falcon",
    "license": "proprietary",
    "type": "project",
    "autoload": {
        "psr-4": {
            "": "src/"
        },
        "classmap": [
            "app/AppKernel.php",
            "app/AppCache.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=5.5.9",
        "symfony/symfony": "3.*",
        "doctrine/orm": "^2.5",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/doctrine-cache-bundle": "^1.2",
        "symfony/swiftmailer-bundle": "^2.3",
        "symfony/monolog-bundle": "^2.8",
        "sensio/distribution-bundle": "^5.0",
        "sensio/framework-extra-bundle": "^3.0.2",
        "sensio/generator-bundle": "^3.0",
        "doctrine/doctrine-migrations-bundle": "^1.0",
        "sonata-project/admin-bundle": "3.*",
        "sonata-project/doctrine-orm-admin-bundle": "3.*",
        "ext-mbstring": "*",
        "ext-intl": "*",
        "sonata-project/seo-bundle": "^2.0",
        "friendsofsymfony/user-bundle": "~2.0",
        "symfony/assetic-bundle": "^2.8",
        "leafo/scssphp": "^0.6.6",
        "patchwork/jsqueeze": "^2.0",
        "aws/aws-sdk-php": "^3.20",
        "vich/uploader-bundle": "^1.4",
        "oneup/flysystem-bundle": "^1.7",
        "league/flysystem-aws-s3-v3": "^1.0",
        "stof/doctrine-extensions-bundle": "^1.2",
        "friendsofsymfony/elastica-bundle": "dev-master",
        "symfony-cmf/routing-bundle": "dev-master",
        "liip/imagine-bundle": "^1.6",
        "presta/sitemap-bundle": "dev-master",
        "knpuniversity/oauth2-client-bundle": "^1.9",
        "league/oauth2-facebook": "^2.0",
        "league/oauth2-google": "^2.0",
        "hwi/oauth-bundle": "^0.5.3",
        "white-october/pagerfanta-bundle": "^1.0",
        "uecode/qpush-bundle": "~2.3.0"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^3.0"
    },
    "scripts": {
        "post-install-cmd": [
            "cp ../parameters.yml ./app/config/",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget",
            "rm -rf web/compiled/",
            "cp -R web/bundles/app/ web/compiled"
        ],
        "post-update-cmd": [
            "cp ../parameters.yml ./app/config/",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget",
            "rm -rf web/compiled/",
            "cp -R web/bundles/app/ web/compiled"
        ],
        "compile": [
            "bin/console assetic:dump --env=dev"
        ]
    },
    "config": {
        "platform": {
            "php": "7.0"
        }
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "web",
        "symfony-tests-dir": "tests"
    }
}
